{
	"name": "cba-vscode",
	"displayName": "Coding Behavior Analysis",
	"publisher": "Broccoli",
	"description": "a tool analyzing your coding behavior",
	"repository": {
		"type": "git",
		"url": "https://github.com/NJUBroccoli/Coding-Behavior-Analysis/"
	},
	"license": "MIT",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.CBA"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.CBA",
				"title": "CBA"
			}
		],
		"keybindings": [
			{
				"command": "cba.evaluation",
				"key": "ctrl+shift+A"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"chart.js": "^2.8.0"
	}
}
